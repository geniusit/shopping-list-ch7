{
  "name": "shopping-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "lint": "vue-cli-service lint",
    "server": "json-server --watch server/db.json"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "karma": "^4.1.0",
    "underscore": "^1.9.1",
    "vue": "^2.6.6",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.14.2",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "vue-resource": "^1.5.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
